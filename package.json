{
  "name": "http-speed-meter",
  "description": "Compare the speed of multiple http clients",
  "version": "1.0.0",
  "repository": "homer0/http-speed-meter",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "keywords": [
    "fetch",
    "http",
    "speed",
    "meter"
  ],
  "dependencies": {
    "chalk": "^5.2.0",
    "yargs": "^17.6.2",
    "shelljs": "0.8.5",
    "pretty-ms": "^8.0.0",
    "cli-spinner": "0.2.10",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "node-fetch": "3.3.0",
    "axios": "1.2.6",
    "got": "12.5.3",
    "superagent": "8.0.9",
    "reqwest": "2.0.5",
    "xhr2": "0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@homer0/eslint-plugin": "^10.0.2",
    "@homer0/prettier-config": "^1.1.2",
    "@homer0/prettier-plugin-jsdoc": "^6.0.1",
    "docdash": "homer0/docdash#semver:^2.1.0",
    "eslint": "8.33.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.4.1",
    "jsdoc": "^4.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=20 <21"
  },
  "main": "src/index.js",
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "start": "./utils/scripts/start",
    "mock": "npm start -- --mock=./mock.json",
    "test": "./utils/scripts/test",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "docs": "./utils/scripts/docs",
    "results": "./utils/scripts/write-results"
  }
}
