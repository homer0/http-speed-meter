    "yargs": "^17.7.2",
    "shelljs": "0.8.5",
    "pretty-ms": "^9.1.0",
    "cli-spinner": "0.2.10",
    "node-fetch": "3.3.2",
    "axios": "1.7.7",
    "gaxios": "^6.7.1",
    "got": "14.4.2",
    "ky": "^1.7.2",
    "superagent": "10.1.0",
    "reqwest": "2.0.5",
    "undici": "^6.21.1",
    "xhr2": "0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@homer0/eslint-plugin": "^12.0.5",
    "@homer0/prettier-config": "^1.1.3",
    "@homer0/prettier-plugin-jsdoc": "^9.0.2",
