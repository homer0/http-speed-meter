  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "keywords": [
    "fetch",
    "http",
    "speed",
    "meter"
  ],
  "type": "module",
  "dependencies": {
    "chalk": "^5.6.2",
    "yargs": "^18.0.0",
    "shelljs": "0.10.0",
    "pretty-ms": "^9.3.0",
    "cli-spinner": "0.2.10",
    "node-fetch": "^3.3.2",
    "axios": "^1.13.2",
    "gaxios": "^7.1.3",
    "got": "14.6.5",
    "ky": "^1.14.0",
    "superagent": "10.2.3",
    "reqwest": "2.0.5",
    "undici": "^7.16.0",
    "xhr2": "0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@homer0/eslint-plugin": "^14.2.2",
    "@homer0/prettier-config": "^2.0.0",
    "@homer0/prettier-plugin-jsdoc": "^11.0.1",
    "docdash": "homer0/docdash#semver:^2.1.2",
    "eslint": "9.39.1",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4"
  },
  "engine-strict": true,
  "engines": {
